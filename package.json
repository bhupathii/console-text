{
  "name": "console-text",
  "version": "1.0.0",
  "description": "Real-time Telegram alerts for developers - Console.text enhances logging with instant notifications",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=packages/dashboard",
    "build": "npm run build --workspace=packages/dashboard",
    "start": "npm run start --workspace=packages/dashboard",
    "build:client": "npm run build --workspace=packages/client",
    "publish:client": "npm run publish --workspace=packages/client",
    "lint": "npm run lint --workspaces",
    "type-check": "npm run type-check --workspaces"
  },
  "keywords": [
    "console",
    "logging",
    "telegram",
    "alerts",
    "monitoring",
    "developer-tools",
    "notifications"
  ],
  "author": "Console.text Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/console-text/console-text"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "dotenv": "^16.5.0"
  }
}
